{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "mphasis-task25",
    "containerDefinitions": [
        {
            "name": "mphasis-task25",
            "image": "docker.io/httpd",
            "essential": true,
            "portMappings": [
                {
                    "name": "mphasis-task25",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ]
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "3 GB",
    "cpu": "1 vCPU",
    "executionRoleArn": "arn:aws:iam::832249129843:role/ecsTaskExecutionRole"
}
